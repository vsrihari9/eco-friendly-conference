        "parameters": [],
        "responses": {
          "200": {
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "example": "API Micro-Service"
                },
                "status": {
                  "type": "string",
                  "example": "Ready"
                },
                "version": {
                  "type": "string",
                  "example": "0.0"
                },
                "started": {
                  "type": "string",
                  "example": "2020-05-31T04:01:54.315Z"
                },
                "uptime": {
                  "type": "number",
                  "example": 0.231
                },
                "currentTime": {
                  "type": "string",
                  "example": "2020-05-31T04:01:54.546Z"
                }
              }
            }
          },
          "500": {
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "Failed to retrieve healthcheck status"
                }
              }
            }
          }
        },
        "produces": [
          "application/json"
        ]
      }
